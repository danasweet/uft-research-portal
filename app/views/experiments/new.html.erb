<%= form_for @experiment, url: proposal_experiments_path do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br />

  <%= f.label :description %>:
  <%= f.text_field :description %><br />

  <%= f.label :equipment %>:
  <%= f.text_field :equipment %><br />

  <%= f.label :procedure %>:
  <%= f.text_field :procedure %><br />

  <%= f.submit %>
<% end %>